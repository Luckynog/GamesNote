<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.games.note.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/gameicon_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/gameicon_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GamesNote">
        <activity
            android:name="br.games.note.app.Views.JogoFavorito.EditJogosActivity"
            android:exported="false" />
        <activity
            android:name="br.games.note.app.Views.Empresa.EditEmpresasActivity"
            android:exported="false" />
        <activity
            android:name="br.games.note.app.Views.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>